<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Menu_Registrar">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ivteclogo1"
        android:background="@mipmap/teclogo2"
        android:layout_centerHorizontal="true"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/bVerr"
        android:text="1. VER REGISTROS"
        android:layout_below="@+id/ivteclogo1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="70dp"
        android:background="@color/negro"
        android:textColor="@color/plata"
        android:textSize="20dp"
        android:paddingLeft="24.5dp"
        android:paddingRight="25dp"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/bRegistrar"
        android:text="2. REGISTRAR"
        android:layout_below="@+id/bVerr"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="80dp"
        android:background="@color/negro"
        android:textColor="@color/plata"
        android:textSize="20dp"
        android:paddingLeft="45dp"
        android:paddingRight="45dp"/>

</RelativeLayout>