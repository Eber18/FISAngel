<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Menu_mantenimiento">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ivteclogo"
        android:background="@mipmap/teclogo2"
        android:layout_centerHorizontal="true"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/bVer"
        android:text="1. Ver"
        android:layout_below="@+id/ivteclogo"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="70dp"
        android:background="@color/negro"
        android:textColor="@color/plata"
        android:textSize="20dp"
        android:paddingLeft="49dp"
        android:paddingRight="50dp"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/bMante"
        android:text="2. DAR ALTA"
        android:layout_below="@+id/bVer"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="100dp"
        android:background="@color/negro"
        android:textColor="@color/plata"
        android:textSize="20dp"
        android:paddingLeft="21.7dp"
        android:paddingRight="20.7dp"/>



</RelativeLayout>