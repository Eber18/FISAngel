<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Menu_inventario">
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ivteclogo"
        android:background="@mipmap/teclogo2"
        android:layout_centerHorizontal="true"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/bVerin"
        android:text="1. Ver"
        android:layout_below="@+id/ivteclogo"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="70dp"
        android:background="@color/negro"
        android:textColor="@color/plata"
        android:textSize="20dp"
        android:paddingLeft="49dp"
        android:paddingRight="49dp"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/bModi"
        android:text="2. Modificar"
        android:layout_below="@+id/bVerin"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="100dp"
        android:background="@color/negro"
        android:textColor="@color/plata"
        android:textSize="20dp"
        android:paddingLeft="15dp"
        android:paddingRight="15.24dp"/>




</RelativeLayout>